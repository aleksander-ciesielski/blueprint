{
  "extends": "../../tsconfig.base.json",
  "include": [
    "./src"
  ],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "esnext",
    "outDir": "./dist/",
    "baseUrl": "./src",
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": false,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ]
}
